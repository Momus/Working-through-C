#include <stdio.h>

#define LOWER 0
#define UPPER 300
#define STEP  20

/* print Fahrenheit-Celsius table */

main()
{
  /* I want to see more values, let's see if we can change LOWER here */

  LOWER = -100 ;

  int fahr;

  for (fahr = LOWER; fahr <= UPPER; fahr = fahr +STEP)
    printf("%3d %6.1\n" , fahr, (5.0/9.0)*(fahr-32)) ;
}
